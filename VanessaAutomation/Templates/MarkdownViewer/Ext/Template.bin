  if (librariesInitialized) return;
  registerLanguages();
  configureMarked(marked);
  librariesInitialized = true;
}
function renderMarkdown(markdownString, containerId) {
  try {
    initializeLibraries();
    const html2 = marked.parse(markdownString);
    const processedHtml = highlightCodeBlocks(html2);
    const finalHtml = createContentHTML(processedHtml);
    let targetContainer = null;
    if (containerId) {
      targetContainer = document.getElementById(containerId);
      if (!targetContainer) {
        logError("Контейнер не найден", new Error(`Элемент с ID "${containerId}" не найден`));
        return;
      }
    } else {
      targetContainer = document.getElementById("markdownContent");
      if (!targetContainer) {
        logError("Основной контейнер не найден", new Error('Элемент с ID "markdownContent" не найден'));
        return;
      }
    }
    targetContainer.innerHTML = finalHtml;
    if (!containerId || containerId === "markdownContent") {
      const loading = document.getElementById("loading");
      const emptyState = document.getElementById("emptyState");
      if (loading) {
        loading.style.display = "none";
      }
      if (emptyState) {
        emptyState.style.display = "none";
      }
    }
  } catch (error2) {
    logError("Ошибка рендеринга markdown", error2);
    const errorHtml = createErrorHTML("Ошибка рендеринга markdown: " + error2.message);
    let targetContainer = null;
    if (containerId) {
      targetContainer = document.getElementById(containerId);
    } else {
      targetContainer = document.getElementById("markdownContent");
    }
    if (targetContainer) {
      targetContainer.innerHTML = errorHtml;
    }
  }
}
function clearMarkdown(containerId) {
  let targetContainer = null;
  if (containerId) {
    targetContainer = document.getElementById(containerId);
    if (!targetContainer) {
      logError("Контейнер не найден", new Error(`Элемент с ID "${containerId}" не найден`));
      return;
    }
  } else {
    targetContainer = document.getElementById("markdownContent");
    if (!targetContainer) {
      logError("Основной контейнер не найден", new Error('Элемент с ID "markdownContent" не найден'));
      return;
    }
  }
  targetContainer.innerHTML = "";
  if (!containerId || containerId === "markdownContent") {
    const emptyState = document.getElementById("emptyState");
    if (emptyState) {
      emptyState.style.display = "block";
    }
  }
}
function isMarkdownSupported() {
  return typeof marked !== "undefined";
}
if (typeof window !== "undefined") {
  window.renderMarkdown = renderMarkdown;
  window.clearMarkdown = clearMarkdown;
  window.isMarkdownSupported = isMarkdownSupported;
}
class MarkdownViewer {
  constructor() {
    if (!isMarkdownSupported()) {
      console.error("Markdown не поддерживается в данной среде");
      return;
    }
    console.log("Markdown Viewer инициализирован");
    console.log("Доступные функции:");
    console.log("- renderMarkdown(markdownString, containerId?)");
    console.log("- clearMarkdown(containerId)");
    console.log("- isMarkdownSupported()");
    console.log("renderMarkdown доступна:", typeof renderMarkdown === "function");
    console.log("clearMarkdown доступна:", typeof clearMarkdown === "function");
  }
}
document.addEventListener("DOMContentLoaded", () => {
  new MarkdownViewer();
});

